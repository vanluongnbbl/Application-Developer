<!doctype html>
<html lang="en">
  <head>
    <title>Staff</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <script src="https://kit.fontawesome.com/3fa7701fe0.js" crossorigin="anonymous"></script>

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  </head>
  <body>
    <br><br>
    <div class="container">
       <br> <br> 
        <div class="row text-center">
            <div class="col-md-5 btn btn-info "><a href="/admin/add/topic" style="color: antiquewhite;"><i class="fas fa-plus-square"></i> Create Topic</a></div>
            <div class="col-md-2"></div>
            <div class="col-md-5 btn btn-warning"><a href="/view/list/topic" style="color: antiquewhite;"><i class="fas fa-eye"></i> View list of Topic</a></div>
        </div>
        <hr>
        <div class="row text-center">
            <div class="col-md-5 btn btn-primary "><a href="/admin/add/coach" style="color: antiquewhite;"><i class="fas fa-plus-square"></i> Create Coach</a></div>
            <div class="col-md-2"></div>
            <div class="col-md-5 btn btn-dark"><a href="/view/list/coach" style="color: antiquewhite;"><i class="fas fa-eye"></i> View list of Coach</a> </div>
        </div>
        <hr>
        <div class="row text-center">
            <div class="col-md-5 btn btn-danger "><a href="/admin/add/trainee" style="color: antiquewhite;"><i class="fas fa-plus-square"></i> Create Trainee</a></div>
            <div class="col-md-2"></div>
            <div class="col-md-5 btn btn-success"><a href="/view/list/trainee" style="color: antiquewhite;"><i class="fas fa-eye"></i> View list of Trainee</a> </div>
        </div>
        <hr>
        <div class="row text-center">
          <div class="col-md-5 btn btn-secondary "><a href="/admin/add/course" style="color: antiquewhite;"><i class="fas fa-plus-square"></i> Create Course</a></div>
          <div class="col-md-2"></div>
          <div class="col-md-5 btn btn-danger"><a href="/view/list/course" style="color: antiquewhite;"><i class="fas fa-eye"></i> View list of Course</a></div>
      </div>
  
    </div>
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
