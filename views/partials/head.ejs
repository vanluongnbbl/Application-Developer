        <button class="navbar-toggler d-lg-none" type="button" data-toggle="collapse" data-target="#collapsibleNavId" aria-controls="collapsibleNavId"
            aria-expanded="false" aria-label="Toggle navigation"></button>
        <div class="collapse navbar-collapse" id="collapsibleNavId">
            <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                <li class="nav-item active">
                    <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
                </li>
                <!-- <li class="nav-item">
                    <a class="nav-link" href="/view/list/course">Coach</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/view/list/course">Course</a>
                </li> -->
                
            </ul>
            <% if(!locals.result) { %>
                <div class="btn btn-success"><a href="/login" style="color: antiquewhite;">Login</a></div>          
               <% } else { %>
                 <div class="btn btn-success"><a href="/logout" style="color: antiquewhite;">Logout</a></div>
              <% } %>
        </div>
    </nav>
</div>
</a>
